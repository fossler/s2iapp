<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="urn:com:adcubum:syrius:api:bestandsverw:vertragsverw:auskunft:data:versichertesobjekt:v1:schema"
            xmlns:tns="urn:com:adcubum:syrius:api:bestandsverw:vertragsverw:auskunft:data:versichertesobjekt:v1:schema"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:st="urn:com:adcubum:syrius:api:common:simpleTypes:v1"
            xmlns:ct="urn:com:adcubum:syrius:api:common:types:v1"
            xmlns:pc="urn:com:adcubum:syrius:api:bestandsverw:common:codes:v1"
            xmlns:pi="urn:com:adcubum:syrius:api:bestandsverw:common:identifier:v1"
            version="1.2.0"
            elementFormDefault="qualified">

    <xsd:import namespace="urn:com:adcubum:syrius:api:common:simpleTypes:v1" schemaLocation="../../../../../../../common/simpleTypes/v1/simpleTypes.xsd"/>
    <xsd:import namespace="urn:com:adcubum:syrius:api:common:types:v1" schemaLocation="../../../../../../../common/types/v1/types.xsd"/>
    <xsd:import namespace="urn:com:adcubum:syrius:api:bestandsverw:common:codes:v1" schemaLocation="../../../../../../common/codes/v1/codes.xsd"/>
    <xsd:import namespace="urn:com:adcubum:syrius:api:bestandsverw:common:identifier:v1" schemaLocation="../../../../../../common/identifier/v1/identifier.xsd"/>

    <xsd:element name="getVersichertesObjektRequest" type="tns:GetVersichertesObjektRequestType">
		<xsd:annotation>
			<xsd:documentation>Request zur Ermittlung der versicherten Objekte zu den übergebenen VersichertesObjektIDs</xsd:documentation>
		</xsd:annotation>
    </xsd:element>

    <xsd:element name="getVersichertesObjektResponse" type="tns:GetVersichertesObjektResponseType">
		<xsd:annotation>
			<xsd:documentation>Response zur Ermittlung der versicherten Objekte zu den übergebenen VersichertesObjektIDs</xsd:documentation>
		</xsd:annotation>
    </xsd:element>

	<xsd:complexType name="GetVersichertesObjektRequestType">
	    <xsd:annotation>
	       <xsd:documentation>Request zum Lesen von versicherten Objekten</xsd:documentation>
	    </xsd:annotation>
		<xsd:sequence>
			<xsd:element name="versichertesObjektId" type="pi:WsVersichertesObjektIdType" minOccurs="1" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Technischer Schlüssel vom versicherten Objekt.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="stichdatum" type="st:DateType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation xml:lang="de">Stichdatum, zu dem das versicherte Objekt ermittelt werden soll. Wird kein Datum übergeben, wird das Tagesdatum herangezogen.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="GetVersichertesObjektResponseType">
	   <xsd:annotation>
	       <xsd:documentation>Response zum Lesen von versicherten Objekten.</xsd:documentation>
	    </xsd:annotation>
		<xsd:sequence>
			<xsd:element name="versichertesObjekt" type="tns:WsVersichertesObjektType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
    				<xsd:documentation>Versichertes Objekt</xsd:documentation>
    			</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="searchVersichertesObjektForVertragRequest" type="tns:SearchVersichertesObjektForVertragRequestType">
		<xsd:annotation>
			<xsd:documentation>Request zur Ermittlung der versicherten Objekte zu einem Vertrag.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

    <xsd:element name="searchVersichertesObjektForVertragResponse" type="tns:SearchVersichertesObjektForVertragResponseType">
        <xsd:annotation>
            <xsd:documentation>Response zur Ermittlung der versicherten Objekte zu einem Vertrag.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>

    <xsd:complexType name="SearchVersichertesObjektForVertragRequestType">
        <xsd:annotation>
           <xsd:documentation>Request zur Suche von versicherten Objekten zu einem Vertrag</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="vertragId" type="pi:WsVertragIdType" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Technischer Schlüssel vom Vertrag.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="stichdatum" type="st:DateType" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="de">Stichdatum, zu dem die versicherten Objekte ermittelt werden sollen. Wird kein Datum übergeben, wird das Tagesdatum herangezogen.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SearchVersichertesObjektForVertragResponseType">
       <xsd:annotation>
           <xsd:documentation>Response zur Suche von versicherten Objekten zu einem Vertrag</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="versichertesObjektId" type="pi:WsVersichertesObjektIdType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Technischer Schlüssel des versicherten Objekts</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="searchVersichertesObjektForPartnerRequest" type="tns:SearchVersichertesObjektForPartnerRequestType">
        <xsd:annotation>
            <xsd:documentation>Request zur Suche von versicherten Objekten zu einem Partner.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>

    <xsd:element name="searchVersichertesObjektForPartnerResponse" type="tns:SearchVersichertesObjektForPartnerResponseType">
		<xsd:annotation>
			<xsd:documentation>Response zur Ermittlung der versicherten Objekte zu einem Partner.</xsd:documentation>
		</xsd:annotation>
    </xsd:element>

	<xsd:complexType name="SearchVersichertesObjektForPartnerRequestType">
	    <xsd:annotation>
	       <xsd:documentation>Request zur Suche von versicherten Objekten zu einem Partner</xsd:documentation>
	    </xsd:annotation>
		<xsd:sequence>
			<xsd:element name="partnerId" type="pi:WsPartnerIdType" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Technischer Schlüssel vom Partner.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="stichdatum" type="st:DateType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation xml:lang="de">Stichdatum, zu dem die versicherten Objekte ermittelt werden sollen. Wird kein Datum übergeben, wird das Tagesdatum herangezogen.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="SearchVersichertesObjektForPartnerResponseType">
	   <xsd:annotation>
	       <xsd:documentation>Response zur Suche von versicherten Objekten zu einem Partner</xsd:documentation>
	    </xsd:annotation>
		<xsd:sequence>
            <xsd:element name="versichertesObjektId" type="pi:WsVersichertesObjektIdType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Technischer Schlüssel des versicherten Objekts</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
		</xsd:sequence>
	</xsd:complexType>

    <xsd:complexType name="WsVersichertesObjektType">
    	<xsd:annotation>
    		<xsd:documentation>Versichertes Objekt</xsd:documentation>
    	</xsd:annotation>
        <xsd:complexContent>
        	<xsd:annotation>
    			<xsd:documentation>Versichertes Objekt</xsd:documentation>
    		</xsd:annotation>
            <xsd:extension base="ct:WsHistObjectType">
                <xsd:sequence>
                    <xsd:element name="id" type="pi:WsVersichertesObjektIdType" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Id eines Versicherten Objekts.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="bezeichnung" type="st:StringType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Bezeichnung des versicherten Objekts.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="praemientotal" type="st:DecimalType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Prämientotal des Versicherten Objekts.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="versicherbaresObjekt" type="tns:WsVersicherbaresObjektType" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Referenz auf das versicherbare Objekt.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="vertragsprodukte" type="tns:WsVertragsproduktType" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>Vertragsprodukte des versicherten Objektes.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="WsVersichertesObjektKopfType">
    	<xsd:annotation>
    		<xsd:documentation>Versichertes Objekt Kopf</xsd:documentation>
    	</xsd:annotation>
        <xsd:complexContent>
        	<xsd:annotation>
    			<xsd:documentation>Versichertes Objekt</xsd:documentation>
    		</xsd:annotation>
            <xsd:extension base="ct:WsHistObjectType">
                <xsd:sequence>
                    <xsd:element name="id" type="pi:WsVersichertesObjektIdType" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Id eines Versicherten Objekts.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="bezeichnung" type="st:StringType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Bezeichnung des versicherten Objekts.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="praemientotal" type="st:DecimalType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Prämientotal des Versicherten Objekts.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="versicherbaresObjekt" type="tns:WsVersicherbaresObjektType" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Referenz auf das versicherbare Objekt.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="vertragsproduktId" type="pi:WsVertragsproduktIdType" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>Technische Schlüssel der Vertragsprodukte des versicherten Objektes.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="WsVersicherbaresObjektType">
    	<xsd:annotation>
    		<xsd:documentation>Versicherbares Objekt</xsd:documentation>
    	</xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ct:WsHistObjectType">
                <xsd:sequence>
                    <xsd:element name="id" type="pi:WsVersicherbaresObjektIdType" minOccurs="1" maxOccurs="1">
                   		<xsd:annotation>
    						<xsd:documentation>Versicherbares-Objekt-Id</xsd:documentation>
    					</xsd:annotation>
                   	</xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="WsVersichertePersonType">
    	<xsd:annotation>
    		<xsd:documentation>Versicherte Person</xsd:documentation>
    	</xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:WsVersicherbaresObjektType">
                <xsd:sequence>
                    <xsd:element name="partnerId" type="pi:WsPartnerIdType" minOccurs="1" maxOccurs="1">
                    	<xsd:annotation>
    						<xsd:documentation>Partner-Id</xsd:documentation>
    					</xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="WsVertragsproduktType">
    	<xsd:annotation>
    		<xsd:documentation>Vertragsprodukt</xsd:documentation>
    	</xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ct:WsHistObjectType">
                <xsd:sequence>
                    <xsd:element name="id" type="pi:WsVertragsproduktIdType" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Technischer Schlüssel eines Vertragsproduktes.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="vertragId" type="pi:WsVertragIdType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Technischer Schlüssel des Vertrages.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="produktId" type="pi:WsProduktIdType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Technischer Schlüssel des Produktes.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="praemienzahlerId" type="pi:WsPraemienzahlerIdType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Technischer Schlüssel des Zahlers.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="leistungsforderungEmpfaengerId" type="pi:WsLeistungsforderungEmpfaengerIdType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Technischer Schlüssel des Empfängers der Leistungsforderung.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="leistungszahlungEmpfaengerId" type="pi:WsLeistungszahlungEmpfaengerIdType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Technischer Schlüssel des Empfängers der Leistungszahlung.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="leistungserbringerId" type="pi:WsPartnerIdType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Technischer Schlüssel des Leistungserbringers.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="praemientotal" type="st:DecimalType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Prämientotal des Vertragsprodukts</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="vpLaufzeitBeginn" type="st:DateType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Vertragsprodukt Laufzeitbeginn.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="mindVPLaufzeitEnde" type="st:DateType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Vertragsprodukt Laufzeitende.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="basisProduktbaustein" type="tns:WsBasisProduktbausteinType" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Der Basis-Produktbaustein.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="produktbausteine" type="tns:WsProduktbausteinType" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>Eine Liste von Produktbausteinen.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="rabatt" type="tns:WsRabattType" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>Rabatt des Vertragsprodukts</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="zuschlag" type="tns:WsZuschlagType" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>Zuschlag des Vertragsprodukts</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="reduktion" type="tns:WsReduktionType" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>Reduktion des Vertragsprodukts</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="sistierung" type="tns:WsSistierungType" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>Sistierung des Vertragsprodukts</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="grundpreis" type="st:DecimalType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Grundpreis des Vertragsprodukts</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="verkaufsproduktId" type="pi:WsVerkaufsproduktIdType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Technischer Schlüssel des Verkaufsproduktes.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="WsKriteriumType">
    	<xsd:annotation>
    		<xsd:documentation>Kriterium</xsd:documentation>
    	</xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ct:WsHistObjectType">
                <xsd:sequence>
                    <xsd:element name="id" type="pi:WsKriteriumIdType" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Technischer Schlüssel des Kriteriums.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="kriteriumTyp" type="pc:WsKriteriumTypCodeType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Kriterium Typ Code.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="kriteriumdefinitionTyp" type="pc:WsKriteriumdefinitionTypCodeType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Typ der Kriteriumdefinition.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="isUebersteuert" type="st:BooleanType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Gibt an ob Kriterum uebersteuert ist.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="kriteriumKey" type="st:StringType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Fachlicher Schlüssel des Kriteriums</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="kriteriumdefinitionKey" type="st:StringType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Fachlicher Schlüssel der Definitionskomponente</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="WsCodekriteriumType">
    	<xsd:annotation>
    		<xsd:documentation>Code-Kriterium</xsd:documentation>
    	</xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:WsKriteriumType">
                <xsd:sequence>
                    <xsd:element name="kriteriumCode" type="pi:WsCodeIdType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Code des Kriteriums.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="kriteriumCodeKey" type="st:StringType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Fachlicher Schlüssel des Codekriteriums.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="WsWertkriteriumType">
    	<xsd:annotation>
    		<xsd:documentation>Wertkriterium</xsd:documentation>
    	</xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:WsKriteriumType">
                <xsd:sequence>
                    <xsd:element name="kriteriumWert" type="st:DecimalType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Wert des Kriteriums.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="WsProduktbausteinType">
    	<xsd:annotation>
    		<xsd:documentation>Produktbaustein</xsd:documentation>
    	</xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ct:WsHistObjectType">
                <xsd:sequence>
                    <xsd:element name="id" type="pi:WsProduktbausteinIdType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Technischer Schlüssel des Vertragsprodukt Risiko.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="vertragsproduktId" type="pi:WsVertragsproduktIdType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Technischer Schlüssel des Vertragsproduktes.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="kriterien" type="tns:WsKriteriumType" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>Die Kriterien des Bausteins</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
					<xsd:element name="produktbausteinEndpreis" type="st:DecimalType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Endpreis des Bausteins</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="sistierungen" type="tns:WsSistierungType" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>Die Sistierungen des Bausteins</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="produktRisikoDefId" type="pi:WsProduktRisikoDefIdType" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>Technischer Schlüssel der Produkt-Risiko Definition</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="rabatte" type="tns:WsRabattType" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>Rabatte des Bausteins</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="zuschlaege" type="tns:WsZuschlagType" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>Zuschlaege des Bausteins</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="reduktionen" type="tns:WsReduktionType" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>Reduktionen des Bausteins</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="WsBasisProduktbausteinType">
    	<xsd:annotation>
    		<xsd:documentation>Produktbaustein</xsd:documentation>
    	</xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:WsProduktbausteinType">
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="WsRabattType">
    	<xsd:annotation>
    		<xsd:documentation>Rabatt</xsd:documentation>
    	</xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ct:WsLifecycleObjectType">
                <xsd:sequence>
                    <xsd:element name="id" type="pi:WsPraemienreduktionIdType" minOccurs="1" maxOccurs="1">
