<?xml version="1.0" encoding="utf-8"?>
<xsd:schema
	targetNamespace="urn:com:adcubum:syrius:api:schadenleistungsmgmt:kvuv:rechnung:common:codes:v1"
	xmlns:tns="urn:com:adcubum:syrius:api:schadenleistungsmgmt:kvuv:rechnung:common:codes:v1"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	xmlns:cc="urn:com:adcubum:syrius:api:common:codes:v1"
	elementFormDefault="qualified"
	version="1.0.0">

	<xsd:import namespace="urn:com:adcubum:syrius:api:common:codes:v1"
		schemaLocation="../../../../../../../../../../com/adcubum/syrius/api/common/codes/v1/codes.xsd" />
		

	<xsd:complexType name="WsRechnungStatusCodeType">
		<xsd:annotation>
			<xsd:documentation>Der Status einer Rechnung
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cc:WsCodeType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="WsRechnungSubStatusCodeType">
		<xsd:annotation>
			<xsd:documentation>Der Substatus einer Rechnung
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cc:WsCodeType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="WsBuchungsGrundCodeType">
		<xsd:annotation>
			<xsd:documentation>Der BuchungsGrundCode einer Deckungsposition
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cc:WsCodeType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="WsDiagnosesystemCodeType">
		<xsd:annotation>
			<xsd:documentation>Der DiagnosesystemCode einer Diagnose
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cc:WsCodeType" />
		</xsd:complexContent>
	</xsd:complexType>


</xsd:schema>
