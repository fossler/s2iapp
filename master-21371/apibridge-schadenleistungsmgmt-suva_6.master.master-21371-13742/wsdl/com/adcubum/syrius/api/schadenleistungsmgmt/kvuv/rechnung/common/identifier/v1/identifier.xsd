<?xml version="1.0" encoding="utf-8"?>
<xsd:schema
	targetNamespace="urn:com:adcubum:syrius:api:schadenleistungsmgmt:kvuv:rechnung:common:identifier:v1"
	xmlns:tns="urn:com:adcubum:syrius:api:schadenleistungsmgmt:kvuv:rechnung:common:identifier:v1"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	xmlns:ci="urn:com:adcubum:syrius:api:common:identifier:v1"
	elementFormDefault="qualified"
	version="1.0.0">

	<xsd:import 
		namespace="urn:com:adcubum:syrius:api:common:identifier:v1"
		schemaLocation="../../../../../../../../../../com/adcubum/syrius/api/common/identifier/v1/identifier.xsd" />

	<xsd:complexType name="WsRechnungIdType">
		<xsd:annotation>
			<xsd:documentation>Technischer Schlüssel einer Rechnung
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ci:WsIdType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="WsMandantIdType">
		<xsd:annotation>
			<xsd:documentation>Technischer Schlüssel eines Madanten
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ci:WsIdType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="WsFalldossierIdType">
		<xsd:annotation>
			<xsd:documentation>Technischer Schlüssel eines Falldossiers
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ci:WsIdType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="WsVerguetungIdType">
		<xsd:annotation>
			<xsd:documentation>Technischer Schlüssel eines Vergütung
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ci:WsIdType" />
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="WsDeckungPositionTextIdType">
		<xsd:annotation>
			<xsd:documentation>Technischer Schlüssel eines DeckungPositiontextes 
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ci:WsIdType" />
		</xsd:complexContent>
	</xsd:complexType>
</xsd:schema>
