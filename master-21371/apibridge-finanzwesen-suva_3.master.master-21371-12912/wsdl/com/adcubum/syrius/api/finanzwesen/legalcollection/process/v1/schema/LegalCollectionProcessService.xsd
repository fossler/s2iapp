    </xsd:simpleType>

    <xsd:complexType name="WsImportResponseType">
        <xsd:annotation>
            <xsd:documentation>
                Includes the status of the request.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="meta" type="tns:WsMetaType" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>MetaData of the message.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="statusType" type="tns:WsStatusType" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>ID of the message.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="errorMessages" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>The error messages, if the status isn't ok.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="details" type="tns:WsImportResponseDetailType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>The details of the response.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="WsImportResponseDetailType">
        <xsd:annotation>
            <xsd:documentation>
                Includes the detail status of the request.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="claimDetail" type="tns:WsString40Type" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Identification of the claim detail</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="claimNumber" type="tns:WsString40Type" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Business-Identification of the claim detail</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="claimDetailAncestors" type="tns:WsString40Type" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>In case of a certificate of loss message, this field can contain the ancestor of
                        the claim for a correct assignemt
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="amortization" type="tns:WsString40Type" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Identification of the amortization</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="statusType" type="tns:WsStatusType" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This detail was processed without any errors</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="WsStatusType">
        <xsd:annotation>
            <xsd:documentation>Type of the additional costs</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
            <xsd:enumeration value="OK">
                <xsd:annotation>
                    <xsd:documentation>Status is ok.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="NOK">
                <xsd:annotation>
                    <xsd:documentation>Status is not ok.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="WsTargetType">
